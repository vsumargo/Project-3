    "eslint-plugin-react-hooks": "^4.2.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "aws-sdk": "^2.892.0",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "express": "^4.16.3",
    "express-session": "^1.17.1",
    "if-env": "^1.0.4",
